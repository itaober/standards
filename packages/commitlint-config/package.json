  "scripts": {
    "clear": "rm -rf dist",
    "build": "pnpm clear && rollup -c"
  },
  "license": "MIT",
  "author": "Taober <itaober@gmail.com> (https://github.com/itaober)",
  "homepage": "https://github.com/itaober/standards/tree/main/packages/commitlint-config#readme",
