    "type": "git",
    "url": "https://github.com/itaober/standards/tree/main/packages/commitlint-config"
  },
  "keywords": [
    "commitlint",
    "commitizen"
  ],
  "peerDependencies": {
    "@commitlint/cli": ">=19.0.0"
  },
  "devDependencies": {
    "@commitlint/types": "^19.5.0",
    "@itaober/tsconfig": "workspace:*",
    "@rollup/plugin-typescript": "^12.1.2",
    "rollup": "^4.29.1",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
