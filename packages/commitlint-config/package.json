{
  "name": "@itaober/commitlint-config",
  "version": "0.1.0-alpha.1",
  "type": "module",
  "description": "Taober's Prettier preset",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
